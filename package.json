    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eleventy-plugin-i18n": "^0.1.3",
    "eleventy-plugin-seo": "^0.5.2",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@11ty/eleventy-img": "^4.0.1",
    "@tailwindcss/cli": "^4.1.18",
    "autoprefixer": "^10.4.23",
