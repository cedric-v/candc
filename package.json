    "build": "cross-env ELEVENTY_ENV=prod npm-run-all build:css build:11ty",
    "build:11ty": "eleventy",
    "build:css": "npx @tailwindcss/cli -i ./src/assets/css/styles.css -o ./_site/assets/css/styles.css --minify",
    "test": "node scripts/smoke-tests.js"
  },
